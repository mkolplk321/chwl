<template name="m-team">
    <view bindtap="viewTeam" data-id="{{team.id}}" data-title="{{team.title}}">
        <view class="jchd1_t">
            <image alt="" src="{{team.image}}"></image>
        </view>
        <view class="jchd1_b">
            <view class="jchd1_b1">
                <view class="jchd1_b1_l">
          {{team.title}}
        </view>
                <view class="jchd1_b1_r">
          ￥{{team.price}}元
        </view>
            </view>
        </view>
        <view class="jchd1_b">
            <view class="jchd1_b1">
                <view class="jchd1_b1_l">
                    <text>开抢时间： {{team.qiang_begin_time}} </text>
                </view>
            </view>
        </view>
        <view class="kong"></view>
    </view>
</template>
