    right: 24rpx;
}

.share-text {
    color: #06c1b6;
    font-size: 24rpx;
}

rich-text .rich-img {
    width: 100%;
    height: auto;
}

.recommend {
    border-bottom: 1px #e7e4dd solid;
}

.recommend .header {
    margin: 10px;
    color: #999;
    font-size: 15px;
}

.recommend .team {
    border-top: 1px #e7e4dd solid;
    margin-left: 20rpx;
}

.recommend .team image {
    margin: 10px;
    width: 200rpx;
    height: 140rpx;
