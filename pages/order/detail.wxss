.me {
    height: 100%;
}

.hongbao {
    position: fixed;
    top: 700rpx;
    right: 10rpx;
    z-index: 2;
}

.hongbaos {
    position: fixed;
    top: 450rpx;
    right: 20%;
    background: rgba(0,0,0,0.4);
    overflow: hidden;
    width: 60%;
}

.hongbaos_text {
    font-size: 24rpx;
}

.page {
    position: absolute;
    z-index: 1;
}

.userinfo {
    position: relative;
    width: 750rpx;
    height: 420rpx;
    background: #4bc6d8;
    color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 15px;
}

.userinfo canvas {
    display: block;
    width: 170px;
    height: 170px;
    background: white;
}

.userinfo-nickname {
    color: #fff;
    font-size: 14px;
}

.sc {
    background: #fff;
    width: 100%;
    font-size: 12px;
    line-height: 16px;
    color: #666;
    padding: 4% 0;
}

.sc image {
    width: 24px;
    height: 24px;
    display: inline-block;
}

.bte {
    border-top: 1px solid #eee;
}

.qbdd {
    background: #fff;
    width: 92%;
    font-size: 12px;
    color: #666;
    padding: 4%;
    border-bottom: 1px solid #eee;
}

.qbdd .font_12 {
    width: 260px;
    word-wrap: break-all;
}

.c33 {
    float: left;
    color: #000;
}

contact-button {
    position: relative;
    left: -50px;
    opacity: 0;
}