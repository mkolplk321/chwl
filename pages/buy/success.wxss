

page {
    background-color: #fff;
}

.modal-box {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0px;
    background: rgba(0,0,0,0.4);
    overflow: hidden;
}

.modal-body {
    position: relative;
}

.modal-content {
    width: 80%;
    height: 1800rpx;
    margin-left: 10%;
    margin-top: 10%;
    overflow: hidden;
    border-radius: 10rpx;
}

.modal-content .fenxiang {
    width: 90%;
    height: 40%;
    border-radius: 25rpx;
    margin-left: 5%;
}

.knowBtn {
    float: right;
}

.buttonshare {
    position: relative;
    width: 90%;
    color: red;
    background-color: yellow;
    border-radius: 25rpx;
    margin-left: 5%;
}

.recommend {
    border-bottom: 1px #e7e4dd solid;
    text-align: left;
    margin-top: 30px;
}

.recommend .team {
    border-top: 1px #e7e4dd solid;
    padding-top: 20rpx;
    padding-left: 30rpx;
}

.recommend .header {
    margin: 10px;
    color: #999;
    font-size: 15px;
}

.recommend .team image {
    margin: 10px;
    width: 200rpx;
    height: 140rpx;
}

.recommend .team .title {
    display: inline-block;
    position: absolute;
    left: 280rpx;
    color: #999;
    font-size: 15px;
    line-height: 70rpx;
    margin-top: 30rpx;
}

.recommend .team .price {
    color: #06c1b6;
    display: inline-block;
    position: absolute;
    left: 280rpx;
    margin-top: 100rpx;
    border: 0px;
    font-size: 16px;
}

.recommend .team .sellerprice {
    text-decoration: line-through;
    color: #ccc;
    display: inline-block;
    left: 50rpx;
}